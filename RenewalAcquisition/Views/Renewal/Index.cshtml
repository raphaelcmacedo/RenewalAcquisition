@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, shrink-to-fit=no, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Prion">

    <title>Import Renewal</title>

    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/css/style.css")" rel="stylesheet">
</head>
<body>

    <a data-toggle="modal" data-target="#loading" style="position: absolute; top:0;left:0;">.</a>

    <div class="site-wrapper">

        <div class="site-wrapper-inner">

            <div class="cover-container">

                <div class="masthead clearfix">
                    <div class="inner">
                        <h2 class="masthead-brand" id="remover-alerta">Renewal</h2>

                    </div>
                </div>

                <div class="inner cover">
                    <p class="lead" id="adicionar-alerta">Upload here the vendor's file</p>

                    <div class="input-group">
                        <span class="input-group-btn">
                            <button id="fake-file-button-browse" type="button" class="btn btn-default">
                                <span class="glyphicon glyphicon-file"></span>
                            </button>
                        </span>
                        <input type="file" id="files-input-upload" style="display:none">
                        <input type="text" id="fake-file-input-name" disabled="disabled" placeholder="Select your file" class="form-control">
                        <span class="input-group-btn">
                            <button type="button" class="btn btn-default" disabled="disabled" @*data-toggle="modal" data-target="#myModal"*@ id="fake-file-button-upload">
                                <span class="glyphicon glyphicon-upload"></span>
                            </button>
                        </span>

                    </div>


                    <div class="oaerror danger out" id="alerta">
                        <strong>Exception</strong> - <span id="AlertaMensagem">O sistema exibe um erro com instruções sobre o formato suportado.</span>
                    </div>


                </div>




            </div>

        </div>

    </div>


    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <div class="modal-header">
                    <h2 class="modal-title" id="myModalLabel">Following the first 10 lines </h2>
                    <h2 class="modal-subtitle" id="myModalLabel">Please check the data then hit confirm button</h2>
                </div>
                <div class="modal-body">
                    <div class="linhas_do_arquivo table-responsive">
                        <table class="table table-bordered table-striped ">
                            <tbody id="sampleBody">                              

                            </tbody>
                        </table>
                    </div>


                </div>
                <div class="modal-footer">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="oaerror info in" id="warning" style="display:none;">
                                <strong>Warning</strong> - <span id="warningMessage"></span>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <form class="form-inline">
                                <div class="form-group">
                                    <label for="exampleInputEmail2">Email</label>
                                    <input type="email" class="form-control" id="exampleInputEmail2" placeholder="jane.doe@example.com">
                                </div>
                                <button type="button" class="btn btn-primary" id="confirmBtn">Confirm</button>

                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="loading" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog modal-loading" role="document">
          <div class="loading">
              <div class="signal"></div>
              <h4>LOADING...</h4>
          </div>
        </div>
    </div>



    <input type="hidden" id="async" value="false" />

    <!-- jQuery -->
    <script  src="@Url.Content("~/Scripts/jquery.js")"></script>
    <!-- Bootstrap Core JavaScript -->
    <script  src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
    <!-- Custom-->
    <script  src="@Url.Content("~/Scripts/index.js")"></script>
</body>

</html>
